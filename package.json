    "express-fileupload": "^1.1.6",
    "express-validator": "^6.3.0",
    "google-maps-react": "^2.0.6",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.2",
    "mongoose-seed": "^0.6.0",
    "passport": "^0.4.1",
